{
  "manifest_version": 3,
  "name": "devhttp",
  "version": "1.0",
  "description": "A terminal for the browser",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "icon-2.png",
      "128": "icon-2.png"
    }
  },
  "content_scripts": [
    {
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ]
}
  