{
    "manifest_version": 2,
    "name": "devhttp",
    "version": "1.0",
    "description": "a terminal for the browser",
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": ["<all_urls>"],
    "icons": ["./icon-2.png"]
  }
  